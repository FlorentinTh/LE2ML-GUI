<h1>
  {{title}}
  <span class="badge badge-right">{{total}}</span>
</h1>

<div class="grid-container" id="algorithms">
  <div class="outer-grid">
    <div class="outer-item" id="add">
      <div class="content">
        <i class="fas fa-plus"></i>
        <h3>Add a new algorithm</h3>
      </div>
    </div>
  </div>
  {{> ../../../search/search}}
  {{> ../../../filters/filters}}
  <div class="grid-algos">
    {{#algorithms}}{{> algo-list}}{{/algorithms}}
  </div>
</div>
