{{#each algos as |algo|}}
{{#[utils/compare] algo.enabled '===' true}}
<option value="{{~algo.slug~}}" data-algo="{{~algo._id~}}">{{~algo.label~}}</option>
{{else}}
<option value="{{~algo.slug~}}" data-algo="{{~algo._id~}}" disabled>{{~algo.label~}}</option>
{{/[utils/compare]}}
{{else}}
{{#[utils/compare] loading '===' true}}
<option disabled>Fetching data...</option>
{{/[utils/compare]}}
{{/each}}
