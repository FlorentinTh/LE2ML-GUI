{{#each functions as |function|}}
{{#[utils/compare] function.enabled '===' true}}
{{#[utils/compare] function.default '===' true}}
<option value="{{~function.container~}}.{{~function.slug~}}" selected>{{~function.label~}}</option>
{{else}}
<option value="{{~function.container~}}.{{~function.slug~}}">{{~function.label~}}</option>
{{/[utils/compare]}}
{{else}}
<option value="{{~function.container~}}.{{~function.slug~}}" disabled>{{~function.label~}}</option>
{{/[utils/compare]}}
{{else}}
{{#[utils/compare] loading '===' true}}
<option disabled>Fetching data...</option>
{{/[utils/compare]}}
{{/each}}
