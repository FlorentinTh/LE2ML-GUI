<div class="action action-left">
  <button id="select-toggle" data-toggle='false'>
    <i class="far fa-check-square"></i>
  </button>
</div>
<div class="task-content">
  <h2>{{title}}</h2>
  <section id="feature-list">
    <div class="feature-container" id="time-features">
      <h3>
        Time Domain
        <span class="badge badge-right">{{totalTimeFeatures}}</span>
      </h3>
      <div class="list-container">
        {{#time-features}}{{> feature-list}}{{/time-features}}
      </div>
    </div>
    <div class="feature-container" id="freq-features">
      <h3>
        Frequential Domain
        <span class="badge badge-right">{{totalFreqFeatures}}</span>
      </h3>
      <div class="list-container">
        {{#freq-features}}{{> feature-list}}{{/freq-features}}
      </div>
    </div>
  </section>
</div>
<div class="btn-group-nav">
  <div class="next">
    <button class="primary">
      Next
      <i class="fas fa-arrow-right"></i>
    </button>
  </div>
  <div class="previous">
    <button class="secondary">
      <i class="fas fa-arrow-left"></i>
      Back
    </button>
  </div>
</div>
